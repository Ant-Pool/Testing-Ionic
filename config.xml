<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="com.ionicframework.sampleapp137450" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0"  xmlns:gap = "http://phonegap.com/ns/1.0">
  <name>Tester</name>
  <description>
        Testing Ionic and cordova.
    </description>
  <author email="hi@ionicframework" href="http://ionicframework.com/">
      Bill Frost
    </author>
  
  <content src="index.html"/>
  <access origin="*"/>
